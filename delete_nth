list_1 = [1, 1, 2, 2, 2, 3, 4, 3, 5, 5, 6, 6, 7, 7, 7, 7]
rep = 2

def delete_nth(order, rep):
    list_final = []
    dic = {}
    for n in list_1:
        if n not in dic:
            dic[n] = 1
            list_final.append(n)
        else:
            dic[n] += 1
            if dic[n] <= rep:
                list_final.append(n)
    print(list_final)
    return list_final


delete_nth(list_1, rep)
